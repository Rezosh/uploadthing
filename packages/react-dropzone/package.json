{
  "name": "@uploadthing/react-dropzone",
  "version": "0.0.1",
  "license": "MIT",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/*"
      ]
    }
  },
  "scripts": {
    "lint": "eslint \"**/*.{ts,tsx}\" --max-warnings 0",
    "clean": "git clean -xdf dist node_modules",
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "dev": "tsup --watch"
  },
  "dependencies": {
    "attr-accept": "^2.2.2",
    "file-selector": "^0.6.0"
  },
  "peerDependencies": {
    "react": "^17.0.2 || ^18.0.0"
  },
  "devDependencies": {
    "@types/react": "18.2.8",
    "@types/react-dom": "18.2.4",
    "@uploadthing/eslint-config": "0.2.0",
    "@uploadthing/tsconfig": "0.1.0",
    "@uploadthing/tsup-config": "0.1.0",
    "eslint": "^8.47.0",
    "react": "18.2.0",
    "tsup": "6.7.0",
    "typescript": "^5.1.6",
    "vitest": "^0.30.1"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@uploadthing/eslint-config/base",
      "@uploadthing/eslint-config/react"
    ]
  }
}
